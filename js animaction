      elem.style.transform="translate(0,0)"
      
    }
    function clickme4() {
        let id = null;    
      const elem = document.getElementById("maindiv2");  
      elem.style.transform="translate(330px,330px)"
     
    }
    window.setTimeout(clickme, 2000);
    window.setTimeout(clickme1, 4000);
    window.setTimeout(clickme2,6000);
